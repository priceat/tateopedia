<h1><%= @wiki.title %> collaborators</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
    </tr>
  </thead>

  <tbody>
    <% @collaborators.each do |collaborator| %>
      <tr>
        <td><%= collaborator.user.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% if @wiki.user == current_user %>
<%= link_to 'New Collaborator', new_wiki_collaborator_path %>
<% end %>
<%= link_to 'Back', wikis_path %>