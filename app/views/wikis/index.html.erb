<h1>All wikis</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Date Created</th>
      <th>Wiki Creator</th>
      <th>Last Updated</th>
      <th>Updater</th> 
    </tr>
  </thead>

  <tbody>
    <% @wikis.each do |wiki| %>
      <tr>
        <td><%= link_to wiki.title, wiki %></td>
        <td><%= wiki.created_at %></td>
        <td><%= wiki.user.email %></td>
        <td><%= wiki.updated_at %></td>
        <td><%= wiki.user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate @wikis %>
<%= link_to 'New Wiki', new_wiki_path %>